<option type="enum" id="opt/hpijs-pcl5e-Quality">
  <!-- A multilingual <comments> block can appear here, too;
       it should be treated as documentation for the user. -->
  <arg_longname>
   <en>Resolution, Quality</en>
  </arg_longname>
  <arg_shortname>
   <!-- We do not take "Resolution" as short name here, as the choices -->
   <!-- of this option cannot be mapped to names like "300dpi", -->
   <!-- "600x300dpi", ... and the Adobe PPD specs require such names -->
   <!-- for the choices of the "Resolution" option --> 
   <en>Quality</en><!-- backends only know <en> shortnames! -->
  </arg_shortname>
  <arg_execution>
   <arg_group>Extra</arg_group>
   <arg_order>100</arg_order>
   <arg_spot>B</arg_spot>
   <arg_substitution />
   <arg_proto> -r%s</arg_proto>
  </arg_execution>
  <constraints>
     <constraint sense='true'>
      <driver>hpijs-pcl5e</driver>
      <arg_defval>ev/hpijs-pcl5e-Quality-600GrayscaleK</arg_defval>
     </constraint>
     <constraint sense='true'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Kyocera-FS-6500</printer>
      <arg_defval>ev/hpijs-pcl5e-Quality-300DraftGrayscaleK</arg_defval>
     </constraint>
     <constraint sense='true'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Kyocera-FS-6500plus</printer>
      <arg_defval>ev/hpijs-pcl5e-Quality-300DraftGrayscaleK</arg_defval>
     </constraint>
     <constraint sense='true'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Ricoh-Aficio_401</printer><!-- Ricoh Aficio 401 -->
      <arg_defval>ev/hpijs-pcl5e-Quality-300DraftGrayscaleK</arg_defval>
     </constraint>
     <constraint sense='true'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Oki-Super_6e</printer><!-- Oki Super 6e -->
      <arg_defval>ev/hpijs-pcl5e-Quality-300DraftGrayscaleK</arg_defval>
     </constraint>
     <constraint sense='true'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Oki-OL410e</printer><!-- Oki OL 410e -->
      <arg_defval>ev/hpijs-pcl5e-Quality-300DraftGrayscaleK</arg_defval>
     </constraint>
     <constraint sense='true'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Canon-LBP-430</printer><!-- Canon LBP-430 -->
      <arg_defval>ev/hpijs-pcl5e-Quality-300DraftGrayscaleK</arg_defval>
     </constraint>
  </constraints>
  <enum_vals>
   <enum_val id="ev/hpijs-pcl5e-Quality-300DraftGrayscaleK">
    <ev_longname>
     <en>300 dpi, Draft</en>
    </ev_longname>
    <!-- A multilingual <comments> block can appear here, too;
         it should be treated as documentation for the user. -->
    <ev_shortname>
     <en>300DraftGrayscaleK</en>
     <!-- Until someone tells me how to learn the user locale in
          backends, the shortname must be monolingual in <en>! -->
    </ev_shortname>
    <ev_driverval>300 -sIjsParams=Quality:Quality=1,Quality:ColorMode=0,Quality:MediaType=0,Quality:PenSet=0</ev_driverval>
   </enum_val>
   <enum_val id="ev/hpijs-pcl5e-Quality-300GrayscaleK">
    <ev_longname>
     <en>300 dpi</en>
    </ev_longname>
    <!-- A multilingual <comments> block can appear here, too;
         it should be treated as documentation for the user. -->
    <ev_shortname>
     <en>300GrayscaleK</en>
     <!-- Until someone tells me how to learn the user locale in
          backends, the shortname must be monolingual in <en>! -->
    </ev_shortname>
    <ev_driverval>300 -sIjsParams=Quality:Quality=0,Quality:ColorMode=0,Quality:MediaType=0,Quality:PenSet=0</ev_driverval>
    <constraints>
     <!-- Assume the choice always applies... -->
     <constraint sense='true'>
      <driver>hpijs-pcl5e</driver>
     </constraint>
     <!-- ...except to these: -->
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Kyocera-FS-6500</printer>
     </constraint>
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Kyocera-FS-6500plus</printer>
     </constraint>
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Ricoh-Aficio_401</printer><!-- Ricoh Aficio 401 -->
     </constraint>
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Oki-Super_6e</printer><!-- Oki Super 6e -->
     </constraint>
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Oki-OL410e</printer><!-- Oki OL 410e -->
     </constraint>
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Canon-LBP-430</printer><!-- Canon LBP-430 -->
     </constraint>
    </constraints>
   </enum_val>
   <enum_val id="ev/hpijs-pcl5e-Quality-600GrayscaleK">
    <ev_longname>
     <en>600 dpi</en>
    </ev_longname>
    <!-- A multilingual <comments> block can appear here, too;
         it should be treated as documentation for the user. -->
    <ev_shortname>
     <en>600GrayscaleK</en>
     <!-- Until someone tells me how to learn the user locale in
          backends, the shortname must be monolingual in <en>! -->
    </ev_shortname>
    <ev_driverval>600 -sIjsParams=Quality:Quality=0,Quality:ColorMode=0,Quality:MediaType=0,Quality:PenSet=0</ev_driverval>
    <constraints>
     <!-- Assume the choice always applies... -->
     <constraint sense='true'>
      <driver>hpijs-pcl5e</driver>
     </constraint>
     <!-- ...except to these: -->
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Kyocera-FS-6500</printer>
     </constraint>
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Kyocera-FS-6500plus</printer>
     </constraint>
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Ricoh-Aficio_401</printer><!-- Ricoh Aficio 401 -->
     </constraint>
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Oki-Super_6e</printer><!-- Oki Super 6e -->
     </constraint>
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Oki-OL410e</printer><!-- Oki OL 410e -->
     </constraint>
     <constraint sense='false'>
      <driver>hpijs-pcl5e</driver>
      <printer>printer/Canon-LBP-430</printer><!-- Canon LBP-430 -->
     </constraint>
    </constraints>
   </enum_val>
  </enum_vals>
</option>
