<driver id="driver/appledmp">
 <name>appledmp</name>
 <url>http://www.ghostscript.com/</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=appledmp%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/Apple-Dot_Matrix</id><!-- Apple Dot Matrix -->
  </printer>
 </printers>
</driver>