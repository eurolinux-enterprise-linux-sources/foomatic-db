<driver id="driver/stc1520h.upp">
  <name>stc1520h.upp</name>
  <url>http://www.ghostscript.com/</url>
  <obsolete replace="gutenprint" />
  <thirdpartysupplied />
  <execution>
    <uniprint />
    <prototype>gs @stc1520h.upp -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -dNOINTERPOLATE %A%Z -sOutputFile=- -</prototype>
    <ppdentry>
      *DefaultResolution: 1440x720dpi
    </ppdentry>
  </execution>
  <comments>
    <en>

      UPP files of this driver: stc1520h.upp&lt;p&gt;

    </en>
  </comments>
  <printers>
    <printer>
      <id>printer/Epson-Stylus_Color_1520</id>
    </printer>
    <printer>
      <id>printer/Epson-Stylus_Color_800</id>
      <comments>
        <en>1440x720dpi, 32-bit CMYK, 62-pin, inkjet paper</en>
      </comments>
    </printer>
  </printers>
</driver>
