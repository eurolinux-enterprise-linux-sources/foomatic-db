<driver id="driver/stc300X.upp">
  <name>stc300X.upp</name>
  <url>http://www.ghostscript.com/</url>
  <obsolete replace="gutenprint" />
  <thirdpartysupplied />
  <execution>
    <uniprint />
    <prototype>gs %B -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -dNOINTERPOLATE %A%Z -sOutputFile=- -</prototype>
    <ppdentry>
      *DefaultResolution: 360dpi
    </ppdentry>
  </execution>
  <comments>
    <en>

      UPP files of this driver: stc300bl.upp, stc300bm.upp,
      stc300.upp&lt;p&gt;

    </en>
  </comments>
  <printers>
    <printer>
      <id>printer/Epson-MJ_520C</id>
    </printer>
    <printer>
      <id>printer/Epson-Stylus_Color_300</id>
    </printer>
  </printers>
</driver>
