<driver id="driver/stc200_h.upp">
  <name>stc200_h.upp</name>
  <url>http://www.ghostscript.com/</url>
  <obsolete replace="gutenprint" />
  <thirdpartysupplied />
  <execution>
    <uniprint />
    <prototype>gs @stc200_h.upp -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -dNOINTERPOLATE %A%Z -sOutputFile=- -</prototype>
    <ppdentry>
      *DefaultResolution: 360x720dpi
    </ppdentry>
  </execution>
  <comments>
    <en>

      UPP files of this driver: stc200_h.upp&lt;p&gt;

    </en>
  </comments>
  <printers>
    <printer>
      <id>printer/Epson-Stylus_Color_200</id>
    </printer>
  </printers>
</driver>
