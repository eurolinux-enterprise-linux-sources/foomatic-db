<driver id="driver/stcX.upp">
  <name>stcX.upp</name>
  <url>http://www.ghostscript.com/</url>
  <obsolete replace="gutenprint" />
  <thirdpartysupplied />
  <execution>
    <uniprint />
    <prototype>gs %B -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -dNOINTERPOLATE %A%Z -sOutputFile=- -</prototype>
    <ppdentry>
      *DefaultResolution: 720dpi
    </ppdentry>
  </execution>
  <comments>
    <en>

      UPP files of this driver: stc.upp, stc_h.upp, stc_l.upp&lt;p&gt;

    </en>
  </comments>
  <printers>
    <printer>
      <id>printer/Epson-Stylus_Color</id>
    </printer>
    <printer>
      <id>printer/Epson-Stylus_Color_440</id>
    </printer>
    <printer>
      <id>printer/Epson-Stylus_Color_460</id>
    </printer>
    <printer>
      <id>printer/Epson-Stylus_Color_I</id>
    </printer>
    <printer>
      <id>printer/Epson-Stylus_Color_PRO</id>
    </printer>
  </printers>
</driver>
