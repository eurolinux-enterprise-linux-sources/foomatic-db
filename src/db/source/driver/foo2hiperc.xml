    </printer>
    <printer>
      <id>printer/Oki-C3400</id>
    </printer>
    <printer>
      <id>printer/Oki-C3530_MFP</id>
    </printer>
    <printer>
      <id>printer/Oki-C5100</id>
    </printer>
    <printer>
      <id>printer/Oki-C5150</id>
    </printer>
    <printer>
      <id>printer/Oki-C5200</id>
    </printer>
