<driver id="driver/Postscript-Samsung">
  <name>Postscript-Samsung</name>
  <url>http://www.samsung.com/printer/</url>
  <supplier>Samsung</supplier>
  <manufacturersupplied>Samsung</manufacturersupplied>
  <license>MIT</license>
  <supportcontacts>
    <supportcontact url="http://www.samsung.com/printer/" level="voluntary">Samsung Support</supportcontact>
  </supportcontacts>
  <shortdescription>
    <en>
      PPD files for Samsung's PostScript printers, supplied by Samsung
    </en>
  </shortdescription>
  <packages>
    <package scope="general" fingerprint="https://www.openprinting.org/download/printdriver/keys/openprinting">openprinting-ppds-postscript-samsung</package>
  </packages>
  <functionality>
    <color />
    <text>100</text>
    <lineart>100</lineart>
    <graphics>100</graphics>
    <photo>100</photo>
  </functionality>
  <execution>
    <postscript />
  </execution>
  <comments>
    <en>

      This driver is intended for Samsung PostScript printers and
      MFP devices.

    </en>
  </comments>
</driver>
